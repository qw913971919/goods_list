<view class="goods_list"><block wx:for="{{goodslist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="goods_item"><image src="{{item.img_url}}" mode="widthFix"></image><text class="title">{{item.title}}</text><view class="content"><view class="price"><text class="sell_price">{{"￥"+item.sell_price}}</text><text class="market_price">{{"￥"+item.market_price}}</text></view><view class="stock_quantity"><text>热卖中</text><text>{{"剩"+item.stock_quantity+"件"}}</text></view></view></view></block><button data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e">加载更多</button></view>